{
    "dataset_reader": {
        "type": "universal_dependencies",
        "use_language_specific_pos": false,
        "lazy": true
    },
    "dataset_reader_aux": {
        "type": "mt_aux_reader",
        "lazy": true
    },
    "iterator": {
        "type": "poly_bucket",
        "batch_size": 80,
        "max_instances_in_memory": 8192*4,
        "sorting_keys": [
            [
                "words",
                "num_tokens"
            ]
        ]
    },
    "iterator_aux": {
        "type": "bucket",
        "batch_size": 80,
        "max_instances_in_memory": 8192*4,
        "sorting_keys": [["words", "num_tokens"], ["target_tokens", "num_tokens"]]
    },
    "vocabulary": {
        "directory_path": "/homes/gws/jkasai/data/europarl/samples/vocabulary"
    },
    "model": {
        "type": "parser_mt",
        "arc_representation_dim": 500,
        "dropout": 0.3,
        "encoder": {
            "type": "stacked_bidirectional_lstm",
            "hidden_size": 400,
            "input_size": 300,
            "num_layers": 3,
            "recurrent_dropout_probability": 0.3,
            "use_highway": false
        },
        "encoder_mt": {
            "type": "stacked_bidirectional_lstm",
            "hidden_size": 400,
            "input_size": 800,
            "num_layers": 1,
            "recurrent_dropout_probability": 0.3,
            "use_highway": false
        },
        "projection_dim": 50,
        "attention": {
          "type": "bilinear",
          "vector_dim": 800,
          "matrix_dim": 800
        },
        "initializer": [
            [
                ".*feedforward.*weight",
                {
                    "type": "xavier_uniform"
                }
            ],
            [
                ".*feedforward.*bias",
                {
                    "type": "zero"
                }
            ],
            [
                ".*tag_bilinear.*weight",
                {
                    "type": "xavier_uniform"
                }
            ],
            [
                ".*tag_bilinear.*bias",
                {
                    "type": "zero"
                }
            ],
            [
                ".*weight_ih.*",
                {
                    "type": "xavier_uniform"
                }
            ],
            [
                ".*weight_hh.*",
                {
                    "type": "orthogonal"
                }
            ],
            [
                ".*bias_ih.*",
                {
                    "type": "zero"
                }
            ],
            [
                ".*bias_hh.*",
                {
                    "type": "lstm_hidden_bias"
                }
            ]
        ],
        "input_dropout": 0.3,
        "tag_representation_dim": 100,
        "text_field_embedder": {
            "tokens": {
                "type": "embedding",
                "embedding_dim": 300,
                "pretrained_file": "../multi_allennlp/data/embeddings/eng-cmnt_prefix.test.vec",
                "sparse": true,
                "trainable": true
            }
        },
        "use_mst_decoding_for_validation": true
    },
    "train_data_path": "../multi_allennlp/data/UD_data/de-en/development/",
    "validation_data_path": "../multi_allennlp/data/UD_data/de-en/development/",
    "train_data_path_aux": "/homes/gws/jkasai/data/europarl/samples/data",
    "validation_data_path_aux": "/homes/gws/jkasai/data/europarl/samples/data",
    "trainer": {
        "cuda_device": 0,
        "grad_norm": 5,
        "num_epochs": 80,
        "optimizer": {
            "type": "dense_sparse_adam",
            "betas": [
                0.9,
                0.9
            ]
        },
        "patience": 50,
        "validation_metric": "+LAS"
    },
    "random_seed": 1,
    "numpy_seed": 1,
    "pytorch_seed": 1
}
